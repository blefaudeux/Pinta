{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python",
            "type": "python",
            "request": "launch",
            "stopOnEntry": false,
            "python": "${command:python.interpreterPath}",
            "program": "${file}",
            "cwd": "${workspaceRoot}",
        },
        {
            "name": "VPP",
            "type": "python",
            "request": "launch",
            "stopOnEntry": false,
            "python": "${command:python.interpreterPath}",
            "program": "${workspaceRoot}/train_vpp.py",
            "cwd": "${workspaceRoot}",
            "args": [
                "--data_path",
                "~/Dropbox/Pinta/imoca/HB7_convert/",
                "--settings_path",
                "settings/settings_imoca_prediction.json",
                "--amp"
            ]
        },
        {
            "name": "Polar",
            "type": "python",
            "request": "launch",
            "stopOnEntry": false,
            "python": "${command:python.interpreterPath}",
            "program": "${workspaceRoot}/test_synthetics.py",
            "cwd": "${workspaceRoot}",
        },
        {
            "name": "ConvertCSV",
            "type": "python",
            "request": "launch",
            "stopOnEntry": false,
            "python": "${command:python.interpreterPath}",
            "program": "${workspaceRoot}/convert_data.py",
            "cwd": "${workspaceRoot}",
            "args": [
                "--data_ingestion_path",
                "~/Downloads/test_data/"
            ]
        },
        {
            "name": "ConvertJSON",
            "type": "python",
            "request": "launch",
            "stopOnEntry": false,
            "python": "${command:python.interpreterPath}",
            "program": "${workspaceRoot}/convert_data.py",
            "cwd": "${workspaceRoot}",
            "args": [
                "--data_ingestion_path",
                "/media/lesle/Data/Dropbox/Pinta/imoca/HB7/",
                "--data_export_path",
                "/media/lesle/Data/Dropbox/Pinta/imoca/HB7_convert",
                "--data_lookup_table",
                "lut_imoca.json",
                "--metadata_root",
                "runsDict"
            ]
        },
        {
            "name": "ConvertNMEA",
            "type": "python",
            "request": "launch",
            "stopOnEntry": false,
            "python": "${command:python.interpreterPath}",
            "program": "${workspaceRoot}/convert_data.py",
            "cwd": "${workspaceRoot}",
            "args": [
                "--data_ingestion_path",
                "data"
            ]
        },
    ]
}