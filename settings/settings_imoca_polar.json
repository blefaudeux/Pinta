{
    "inputs": [
        "tws",
        "twa_x",
        "twa_y",
        "heel",
        "leeway",
        "helm"
    ],
    "tuning_inputs": [],
    "outputs": [
        "sog"
    ],
    "transforms": [
        [
            "normalize",
            []
        ],
        [
            "random_flip",
            [
                [
                    "twa_y"
                ],
                0.5
            ]
        ],
        [
            "cut_sequence",
            [
                null,
                -1
            ]
        ],
        [
            "single_precision",
            []
        ]
    ],
    "trunk": {
        "model_type": "dilated_conv",
        "hidden_size": 64,
        "dilated_conv": {
            "dropout": 0.1
        },
        "mlp": {
            "inner_layers": 3
        },
        "rnn": {
            "gru_layers": 2,
            "kernel_sizes": [
                3,
                3
            ]
        },
        "conv": {
            "kernel_sizes": [
                3,
                3
            ]
        }
    },
    "seq_length": 27,
    "data": {
        "train_batch_size": 20000,
        "test_batch_size": 1000,
        "training_ratio": 0.9,
        "shuffle": true,
        "train_workers": 4,
        "test_workers": 1
    },
    "epoch": 1,
    "optim": {
        "name": "adam_w",
        "learning_rate": 0.01,
        "scheduler": "reduce_plateau",
        "scheduler_patience": 20,
        "scheduler_factor": 0.8,\
        "momentum": 0.9
    },
    "log": "pinta_imoca",
    "amp": false
}