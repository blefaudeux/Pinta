{
    "inputs": [
        "tws",
        "twa_x",
        "twa_y"
    ],
    "outputs": [
        "sog"
    ],
    "transforms": [
        [
            "normalize",
            []
        ],
        [
            "random_flip",
            [
                [
                    "twa_y"
                ],
                0.5
            ]
        ],
        [
            "cut_sequence",
            [
                null,
                -1
            ]
        ],
        [
            "single_precision",
            []
        ]
    ],
    "model_type": "mlp",
    "hidden_size": 256,
    "seq_length": 27,
    "data": {
        "train_batch_size": 10000,
        "test_batch_size": 1000,
        "training_ratio": 0.9,
        "shuffle": true,
        "train_workers": 4,
        "test_workers": 1
    },
    "epoch": 10,
    "optim": {
        "name": "adam_w",
        "learning_rate": 0.01,
        "scheduler": "reduce_plateau",
        "momentum": 0.99
    },
    "dilated_conv": {
        "dropout": 0.25
    },
    "mlp": {
        "inner_layers": 3
    },
    "rnn": {
        "gru_layers": 2,
        "kernel_sizes": [
            3,
            3
        ]
    },
    "conv": {
        "kernel_sizes": [
            3,
            3
        ]
    },
    "log": "pinta_imoca",
    "amp": false
}