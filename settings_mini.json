{
    "inputs": [
        "tws",
        "twa_x",
        "twa_y",
        "helm"
    ],
    "outputs": [
        "sog"
    ],
    "transforms": [
        [
            "normalize",
            []
        ],
        [
            "random_flip",
            [
                [
                    "helm",
                    "twa_y"
                ],
                0.5
            ]
        ]
    ],
    "model_type": "dilated_conv",
    "hidden_size": 128,
    "seq_length": 81,
    "training_ratio": 0.9,
    "train_batch_size": 10000,
    "val_batch_size": 1000,
    "epoch": 10,
    "learning_rate": 0.001,
    "dilated_conv": {
        "dropout": 0.25
    },
    "mlp": {
        "inner_layers": 3
    },
    "rnn": {
        "gru_layers": 2,
        "kernel_sizes": [
            3,
            3
        ]
    },
    "conv": {
        "kernel_sizes": [
            3,
            3
        ]
    },
    "log": "pinta_mini",
    "scheduler": "reduce_plateau",
    "amp": false
}